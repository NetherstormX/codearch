<script>
    function myFunction() {
	    var x = document.getElementById("body")
	    if (x.style.fontFamily == "Impact") {
		    x.style.fontFamily = "Arial";
	    } else {
	    x.style.fontFamily = "Impact";
    } }
</script>      
